{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "ios": {
        "simulator": false  
      }
    },
    "preview": {
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "android": { "buildType": "app-bundle" },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://ubcfirstyearlifeapp.onrender.com",
        "EXPO_PUBLIC_APP_SCHEME": "ubcfirstyearlifeapp"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
